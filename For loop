def re(s):
    str  = ""                        #blank string to iterate
    for i in s:
        st = len(s)                  #get lenth of string
        str = i + str                #add single index value as i from s and add str(blabk string)
        st = st+1                    #increase length value 
    return str                       #return str value out of for loop
s = "123"
print(re(s))


